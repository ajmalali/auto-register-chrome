{
  "manifest_version": 2,
  "name": "Auto-Register",
  "version": "3.6",
  "description": "Automate your course registration process",
  "icons": {
    "128": "icons/ks_128.png",
    "48": "icons/ks_48.png",
    "32": "icons/ks_32.png",
    "16": "icons/ks_16.png"
  },
  "browser_action": {
    "default_icon": "icons/ks_16.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": [
      "js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "http://ssbweb.kfupm.edu.sa/PROD8/bwskfreg.P_AltPin"
      ],
      "js": [
        "js/autofill.js"
      ]
    }
  ],
  "permissions": [
    "http://ssbweb.kfupm.edu.sa/PROD8/bwckcoms.P_Regs",
    "storage",
    "tabs"
  ]
}
