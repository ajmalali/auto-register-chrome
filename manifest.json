{
  "manifest_version": 2,
  "name": "AutoRegister",
  "version": "2.1",
  "description": "This extension automates the KFUPM courses registration process",
  "icons": {
    "128": "icons/get_started128.png",
    "48": "icons/get_started48.png",
    "16": "icons/get_started16.png"
  },
  "browser_action": {
    "default_icon": "icons/get_started16.png"
  },
  "options_page": "options.html",
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "web_accessible_resources": ["script.js"],
  "content_scripts": [
    {
      "matches": ["http://ssbweb.kfupm.edu.sa/PROD8/bwskfreg.P_AltPin/*"],
      "js": ["js/autofill.js"]
    },
    {
      "matches": ["http://ssbweb.kfupm.edu.sa/PROD8/bwskfreg.P_AltPin"],
      "js": ["js/termAutoFill.js"]
    }
  ],
  "permissions": [
    "file:///Users/ajmal/Development/Web%20Dev/auto-register-chrome/test.html",
    "storage",
    "tabs"
  ]
}
